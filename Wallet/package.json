{
  "name": "ilia-nodejs-challenge",
  "version": "1.0.0",
  "description": "Challenge made by Marcelo Magalh√£es.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --transpile-only src/index.ts | pino-pretty -c",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test:functional": "jest --projects ./test --runInBand --verbose",
    "test:unit": "jest",
    "test": "npm run lint && npm run style:check && npm run test:unit && npm run test:functional"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marrcelo/ilia-nodejs-challenge.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marrcelo/ilia-nodejs-challenge/issues"
  },
  "homepage": "https://github.com/marrcelo/ilia-nodejs-challenge#readme",
  "dependencies": {
    "body-parser": "^1.19.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.1.7",
    "mongoose-paginate-ts": "^1.2.0",
    "pino": "^7.6.4",
    "pino-http": "^8.4.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.10",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^27.4.7",
    "pino-pretty": "^7.5.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
